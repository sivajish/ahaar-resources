  "cookingProcedures": [
    {
      "procedureName": "",
      "procedureSteps": [
        {
          "stepDescription": "Wash beetroot and peel off the skin. Grate beetroot. I prefer grating as cutting takes long time. Heat oil in a wok. Add mustard and cumin seeds. Allow it to splutter.",
          "stepImages": [
            {
              "ImageUrl": "step_1_1.JPG",
              "ImageAltText": ""
            },
            {
              "ImageUrl": "step_1_2.JPG",
              "ImageAltText": ""
            }
          ]
        },
        {
          "stepDescription": "Add curry leaves and green chilles. Saute for 1 min.Add ginger, fry for 30 sec. Add moong dal, fry for 30 secs. Add onions, fry till it turns into brown. Add turmeric and coriander powder, fry for 1 min.",
          "stepImages": [
            {
              "ImageUrl": "step_2_1.JPG",
              "ImageAltText": ""
            },
            {
              "ImageUrl": "step_2_2.JPG",
              "ImageAltText": ""
            },
            {
              "ImageUrl": "step_2_3.JPG",
              "ImageAltText": ""
            }
          ]
        },
        {
          "stepDescription": "Now add beetroot. Mix it well. Cover and cook for 20 mins. Beetroot becomes soft by now.",
          "stepImages": [
            {
              "ImageUrl": "step_3_1.JPG",
              "ImageAltText": ""
            }
          ]
        },
        {
          "stepDescription": "Once done, garnish coriander leaves. Serve hot with rotis.",
          "stepImages": [
            {
              "ImageUrl": "step_4_1.JPG",
              "ImageAltText": ""
            }
          ]
        }
      ]
    }
